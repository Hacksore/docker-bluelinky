<?xml version="1.0"?>
<Container version="2">
  <Name>bluelinky</Name>
  <Repository>https://github.com/wranglatang/docker-bluelinky</Repository>
  <Registry>https://hub.docker.com/repository/docker/wranglatang/bluelinky</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://github.com/Hacksore/bluelinky</Support>
  <Project>https://github.com/Hacksore/bluelinky</Project>
  <Overview>Unofficial Hyundai Blue Link API (bluelinky)&#xD;
&#xD;
This is a docker version of an API Wrapper for bluelinky. For information or issues, please raise them on the bluelinky repo.</Overview>
  <Category>Other:</Category>
  <WebUI/>
  <TemplateURL/>
  <Icon/>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1584717572</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Description>Unofficial Hyundai Blue Link API (bluelinky)&#xD;
&#xD;
This is a docker version of an API Wrapper for bluelinky. For information or issues, please raise them on the bluelinky repo.</Description>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort/>
        <ContainerPort>8080</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/bluelinky/</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment/>
  <Labels/>
  <Config Name="Configuration Path" Target="/config" Default="" Mode="rw" Description="Container Path: /config" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/appdata/bluelinky/</Config>
  <Config Name="API Port" Target="8080" Default="8080" Mode="tcp" Description="API port number, that you will send post request to." Type="Port" Display="always" Required="true" Mask="false"/>
</Container>